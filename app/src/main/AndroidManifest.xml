<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gestordealmacenamiento">

    <application
        android:allowBackup="true"
        android:icon="@drawable/program_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/program_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.GestorDeAlmacenamiento">

        <activity android:name=".presentation.Presentation"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".session.Login" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".session.Register" />
        <activity android:name=".session.RecoverPWD" />
    </application>

</manifest>